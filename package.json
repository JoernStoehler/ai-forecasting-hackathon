{
  "name": "@ai-forecasting/root",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev -w @ai-forecasting/webapp",
    "build": "npm run build -w @ai-forecasting/webapp",
    "preview": "npm run preview -w @ai-forecasting/webapp",
    "lint": "npm run lint -w @ai-forecasting/webapp",
    "typecheck": "npm run typecheck -w @ai-forecasting/webapp",
    "test": "npm run test -w @ai-forecasting/webapp",
    "check": "npm run check -w @ai-forecasting/webapp",
