{
  "name": "ai-forecasting-hackathon",
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "postCreateCommand": "npm run post-create",
  "postStartCommand": "bash scripts/post-start.sh",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "README.md",
        "AGENTS.md"
      ]
    },
    "vscode": {
      "extensions": [
        "openai.chatgpt",
        "vitest.explorer",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  }
}
